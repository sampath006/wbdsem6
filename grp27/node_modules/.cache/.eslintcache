om 'react-router-dom';\r\nimport ReactPaginate from 'react-paginate';\r\n\r\nexport default function Home() {\r\n    \r\n    //declaring the set and use states\r\n    const [index,setIndex] = useState(0)\r\n    const [posts, setPosts] = useState([]);\r\n    const [events, setEvents] = useState([]);\r\n    const [title, setTitle] = useState(\"\");\r\n\r\n    //fetching the user from the context\r\n    const [users, setUsers] = useState([])\r\n\r\n    //getting the window location\r\n    const {search} = useLocation(); \r\n\r\n    //fetching the blog posts and events and also users\r\n    useEffect(()=>{\r\n        const fetchPosts = async ()=>{\r\n            const res = await axios.get(\"/posts\"+search)\r\n            setPosts(res.data)\r\n        }\r\n        const fetchEvents = async ()=>{\r\n            const res = await axios.get(\"/events\"+search)\r\n            setEvents(res.data)\r\n        }\r\n        const fetchUsers = async() => {\r\n            const res = await axios.get(\"/users\")\r\n            setUsers(res.data)\r\n        }\r\n        fetchPosts();\r\n        fetchEvents();\r\n        fetchUsers();\r\n    },[search])\r\n\r\n    //pagination\r\n    const slice = posts.slice(6*index,6*index+6);\r\n\r\n    //it handles the change functionality\r\n    const handleChange = (event, value) => {\r\n        setIndex(value-1);\r\n      };\r\n    const handleSearch = ()=>{\r\n        try {\r\n            window.location.replace(`/?title=${title}`);\r\n        } catch (err) {\r\n            \r\n        }\r\n    }\r\n    const currentURL = window.location.href\r\n    return (\r\n        <div className='total_home'>\r\n        <NavBar />\r\n        \r\n\r\n        {/* display slider in homepage */}\r\n        {(currentURL===\"http://localhost:3000/\")?(<Header events={events}/>) : ( \" \" )}\r\n         <Sidebar />\r\n\r\n        <div className=\"searchHome\">\r\n            <div className=\"searchTitle\">\r\n                <form onSubmit={handleSearch}>\r\n                    <input className=\"searchByTitle\" name=\"title\" type=\"text\" placeholder=\"Title\"onChange={(e) => setTitle(e.target.value)}></input>\r\n                    <Button type='submit'><SearchIcon style={{color:\"black\", fontSize:40}} /></Button>\r\n                </form> \r\n            </div>\r\n        </div>\r\n\r\n        <div className=\"home\">\r\n            {/* slicing the blogs */}\r\n           <Posts posts = {slice}/>\r\n        </div>\r\n        <div className='paginate'>\r\n            <div className = \"innerPaginate\">\r\n                {/* pagination */}\r\n                <Pagination count={Math.ceil(posts.length/6)} onChange={handleChange}/>\r\n            </div>\r\n        </div>\r\n        <Footer/>\r\n        </div>\r\n    )\r\n}\r\n","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/login/Login.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/register/Register.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/single/Single.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/add/add.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/addEvent/addEvent.js",["132"],"import \"./addEvent.css\"\r\nimport React, { useContext, useState } from \"react\";\r\nimport axios from 'axios';\r\nimport { Context } from \"../../context/Context\";\r\nimport NavBar from \"../../components/navbar/NavBar\";\r\nimport Footer from \"../../components/footer/Footer\"\r\n\r\nexport default function AddEvent() {\r\n\r\n    //declaring the use and set variables\r\n    const [title, setTitle] = useState(\"\");\r\n    const [desc, setDesc] = useState(\"\");\r\n    const [date, setDate] = useState(\"\");\r\n    const [prize, setPrize] = useState(\"\");\r\n    const [venue, setVenue] = useState(\"\");\r\n    const [categories, setCategories] = useState(\"\");\r\n    const [file, setFile] = useState(null);\r\n\r\n    //fetching the user from the context\r\n    const { user } = useContext(Context);\r\n\r\n    //handles the add event functionality\r\n    const handleAdd = async (e) => {\r\n        e.preventDefault();\r\n        const newEvent = {\r\n            username: user.username,\r\n            title,\r\n            desc,\r\n            categories,\r\n            date,\r\n            prize,\r\n            venue,\r\n        }\r\n\r\n        //handles the image file\r\n        if (file) {\r\n            const data = new FormData();\r\n            const filename = Date.now() + file.name;\r\n            data.append(\"name\", filename);\r\n            data.append(\"file\", file);\r\n            newEvent.photo = filename;\r\n            try {\r\n                await axios.post(\"/upload\", data);//uploads the image\r\n            } catch (err) {\r\n\r\n            }\r\n            try {\r\n                const res = await axios.post(\"/events\", newEvent);//post the data to the backend\r\n                window.location.replace(\"/event/\" + res.data._id);//replaces the url\r\n            } catch (err) {\r\n\r\n            }\r\n        }\r\n\r\n    };\r\n\r\n\r\n    return (\r\n        <>\r\n        <NavBar />\r\n        <div className=\"addEve\">\r\n            {/* show image */}\r\n            {file && (\r\n                <img className=\"addImgEve\" src={URL.createObjectURL(file)} alt=\"\" />\r\n            )}\r\n            {/* form data */}\r\n            <form className=\"addFormEve\" onSubmit={handleAdd}>\r\n                <div className=\"addFormGroupEve\">\r\n                    <div className=\"addImageEvent\">\r\n                        {/* add image */}\r\n                        <label >\r\n                            Choose Event Image : <input name=\"image\" type=\"file\" className=\"addInputImgEve\" onChange={(e) => setFile(e.target.files[0])}></input>\r\n                        </label>\r\n                    </div>\r\n                    <br />\r\n                    <p></p>\r\n                    <br />\r\n                    <div className=\"addTitleTextEve\">\r\n                        <label >Add your Title : <input name=\"title\" type=\"text\" placeholder=\"Title\" className=\"addInputEve\" onChange={(e) => setTitle(e.target.value)} ></input></label>\r\n                    </div>\r\n                    <br />\r\n                    {/* add date */}\r\n                    <div className=\"addTitleTextEve\">\r\n                        <label >Date : <input name=\"date\" type=\"date\" className=\"addDateEve\" onChange={(e) => setDate(e.target.value)} ></input></label><br />\r\n                    </div>\r\n                    <div className=\"addTitleTextEve\">\r\n                        <label >Prize Money : <input name=\"prize\" type=\"text\" placeholder=\"$$$\" className=\"addPrizeEve\" onChange={(e) => setPrize(e.target.value)} ></input></label><br />\r\n                    </div>\r\n                    {/* venue */}\r\n                    <div className=\"addVenueTextEve\">\r\n                        <label >Venue : <input name=\"venue\" type=\"text\" placeholder=\"Venue\" className=\"addVenueEve\" onChange={(e) => setVenue(e.target.value)} ></input></label><br />\r\n                    </div>\r\n                </div>\r\n                    {/* description */}\r\n                <div className=\"addFormGroupEve\">\r\n                    <textarea type=\"text\" name=\"para\" placeholder=\" Tell About your Event \" className=\"addTextEve\" onChange={(e) => setDesc(e.target.value)}></textarea>\r\n                </div>\r\n                {/* add event button */}\r\n                <button className=\"addSubmitEve\" type=\"submit\"> ADD EVENT</button>\r\n            </form>\r\n        </div>\r\n        <Footer />\r\n        </>\r\n    )\r\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/adminlogin/adminlogin.js",["133"],"import \"./adminlogin.css\"\r\nimport { Link } from \"react-router-dom\";\r\nimport { useContext,useRef } from \"react\";\r\nimport { Context } from \"../../context/Context\";\r\nimport axios from \"axios\";\r\nimport NavBar from \"../../components/navbar/NavBar\";\r\nimport Footer from \"../../components/footer/Footer\"\r\n\r\nexport default function Login(){\r\n\r\n    //declaring the user and password reference\r\n    const userRef = useRef()\r\n    const passwordRef = useRef()\r\n\r\n    //dispacting the state and action from the context\r\n    const {dispatch, isFetching} = useContext(Context)\r\n\r\n    //handles submit functionality\r\n    const handleSubmit = async (e)=>{\r\n        e.preventDefault();\r\n        dispatch({type:\"LOGIN_START\"});//dispatch login start action\r\n        try {\r\n            const res = await axios.post(\"/auth/adminlogin\",{//post data to login and check\r\n                username: userRef.current.value,\r\n                password: passwordRef.current.value\r\n            });\r\n            dispatch({type:\"LOGIN_SUCCESS\", payload: res.data});//dispatch login succes action\r\n\r\n        } catch (err) {\r\n            dispatch({type:\"LOGIN_FAILURE\"}); //dispatch login failure action\r\n            alert(\"Wrong Credentials\")\r\n        }\r\n    };\r\n\r\n\r\n    return (\r\n        <>\r\n        <NavBar />\r\n        <div className=\"adminlogin\">\r\n            <span className=\"adminloginTitle\">AdminLogin</span>\r\n            {/* login form */}\r\n            <form className=\"adminloginForm\" onSubmit={handleSubmit}>\r\n                {/* username input */}\r\n                <label>Username</label>\r\n                {/* password input */}\r\n                <input type=\"text\" placeholder=\"Enter Your Username\" className=\"adminloginInput\" ref={userRef}/>\r\n                <label>Password</label>\r\n                <input type=\"password\" placeholder=\"Enter Your Password\" className=\"adminloginInput\" ref={passwordRef}/>\r\n                <button className=\"adminloginButton\" type=\"submit\" disabled={isFetching}>Admin Login</button>\r\n            </form>\r\n            <h3 style={{marginTop:\"10px\"}}>If you need the admin access please contact us</h3>\r\n\r\n        </div>\r\n        <Footer />\r\n        </>\r\n    )\r\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/event/Event.js",["134"],"import React, { useEffect, useState } from 'react';\nimport Events from '../../components/events/Events';\nimport SidebarEve from '../../components/eventSidebar/eventSidebar';\nimport \"./event.css\";\nimport axios from \"axios\";\nimport { useLocation,Link } from 'react-router-dom';\nimport NavBar from \"../../components/navbar/NavBar\";\nimport Footer from \"../../components/footer/Footer\"\n\nexport default function Event() {\n\n    //declaring set and use states\n    const [events, setEvents] = useState([]);\n    const {search} = useLocation(); \n\n\n    //fetching the events from the backend\n    useEffect(()=>{\n        const fetchEvents = async ()=>{\n            const res = await axios.get(\"/events\"+search)\n            setEvents(res.data)\n        }\n        fetchEvents();\n    },[search])\n    return (\n        <>\n        <NavBar />\n        <SidebarEve />\n        <div className=\"event\">\n            {/* maps the events like props */}\n           <Events events = {events}/>\n        </div>\n        <div className='eventh3'>\n        <h3>If you want to organise an event please contact us to get the admin access.<br/> Our contact information is provided in the About page. </h3>\n        </div>\n        <Footer />\n        </>\n    )\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/adminpage/AdminPage.js",["135","136"],"import React, { useEffect, useState } from 'react'\r\nimport './adminPage.css'\r\nimport axios from \"axios\"\r\nimport Header from '../../components/header/Header';\r\nimport Events from '../../components/events/Events';\r\nimport { useLocation,Link } from 'react-router-dom';\r\nimport NavBar from \"../../components/navbar/NavBar\";\r\nimport Footer from \"../../components/footer/Footer\";\r\n\r\nexport default function AdminPage(){\r\n\r\n    //declarimg set and use states\r\n    const [posts, setPosts] = useState([]);\r\n    const [events, setEvents] = useState([]);\r\n\r\n    //fetching user from the context\r\n    const [users, setUsers] = useState([])\r\n\r\n    //getting the url location\r\n    const {search} = useLocation(); \r\n\r\n    useEffect(()=>{\r\n        //fetching the blog posts\r\n        const fetchPosts = async ()=>{\r\n            const res = await axios.get(\"/posts\"+search)\r\n            setPosts(res.data)\r\n        }\r\n        //fetching events\r\n        const fetchEvents = async ()=>{\r\n            const res = await axios.get(\"/events\"+search)\r\n            setEvents(res.data)\r\n        }\r\n\r\n        //fetching the users\r\n        const fetchUsers = async() => {\r\n            const res = await axios.get(\"/users\")\r\n            setUsers(res.data)\r\n        }\r\n        fetchPosts();\r\n        fetchEvents();\r\n        fetchUsers();\r\n    },[search])\r\n   \r\n    \r\n    return(\r\n        <>\r\n        <NavBar/>\r\n        <div className = \"adminPage\">\r\n            <h1 className=\"adminHeader\">Admin Page</h1>\r\n                {/* displayes the total blog posts */}\r\n                <div className=\"postsAndBlogsCount\">\r\n                    <span className = \"postsCount\">Total Posts :{posts.length}</span>\r\n                    <span className = \"eventsCount\">Total Events :{events.length}</span>\r\n                </div>\r\n                <Link className=\"link\" to=\"/addEvent\"><button className=\"addEvent\"> Add An Event </button></Link>\r\n                {/* displays the evenst present */}\r\n                <Events events = {events}/>\r\n\r\n                {/* <h1>hello</h1> */}\r\n        </div>\r\n        <Footer/>\r\n        </>\r\n    );\r\n}\r\n","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/about/About.js",["137"],"import React from 'react'\r\nimport './about.css'\r\nimport img3 from \"../../assets/img-3.png\"\r\nimport NavBar from \"../../components/navbar/NavBar\";\r\nimport Footer from \"../../components/footer/Footer\"\r\nexport default function About() {\r\n    return (\r\n        <>\r\n        <NavBar />\r\n        <div className='about-content'>\r\n            {/* about us heading */}\r\n            <h1 className=\"about-heading\">About us</h1>\r\n\r\n            <hr class=\"style-hr\" />\r\n            <span>\r\n                {/* quote in the about page */}\r\n                <div className=\"about-quote\"><q>Blogging is not rocket science. It's about being yourself and   putting what you have into it.</q></div>\r\n                <div className=\"contain\">\r\n                    <div className=\"text-block-left\">\r\n                        {/* content in the about page */}\r\n                        <h2 >Blogs page Project</h2>\r\n                        <p>Blogging one's thoughts, experiences and exhibiting topics helps others to explore a vast knowledge from bloggers . Here  Bloggers can blog their words and share info about various events. <br />\r\n                            We are team of 5 members. We created this page to share their experience in the form\r\n                            of a blogs.\r\n                            <br />Here you can start blogging and share your experience in word format with\r\n                            everyone. Our team details are:\r\n                        </p>\r\n                        <ul className='names'>\r\n                            {/* our project member names */}\r\n                            <li>M Sampath</li>\r\n                            <li>Deepak</li>\r\n                            <li>Aditya Medarametla</li>\r\n                            <li>Bodhi Aditya</li>\r\n                            <li>Hemanth sai</li>\r\n                        </ul>\r\n                    </div>\r\n                    <div className='about-img'>\r\n                        <img src={img3} width={500} height={500}></img>\r\n                    </div>\r\n                </div>\r\n            </span>\r\n        </div>\r\n        <Footer />\r\n        </>\r\n    )\r\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/singleEvent/SingleEvent.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/pages/settings/Settings.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/events/Events.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/header/Header.js",["138","139"],"import React from 'react'\r\nimport \"./header.css\"\r\nimport SliderEvent from '../slider-event/SliderEvent';\r\n\r\nconst colors = [\"#0088FE\", \"#00C49F\", \"#FFBB28\"];\r\n\r\n//Slider delay speed\r\nconst delay = 2000;\r\nexport default function Header({events}) {\r\n\r\n  const [index, setIndex] = React.useState(0);\r\n  const timeoutRef = React.useRef(null);\r\n\r\n  function resetTimeout() {\r\n    if (timeoutRef.current) {\r\n      clearTimeout(timeoutRef.current);\r\n    }\r\n  }\r\n\r\n  React.useEffect(() => {\r\n    resetTimeout();\r\n    timeoutRef.current = setTimeout(\r\n      () =>\r\n        setIndex((prevIndex) =>\r\n          prevIndex === events.length - 1 ? 0 : prevIndex + 1\r\n        ),\r\n      delay\r\n    );\r\n\r\n    return () => {\r\n      resetTimeout();\r\n    };\r\n  }, [index]);\r\n\r\n  return (\r\n    <div className=\"slideshow\">\r\n      <div\r\n        className=\"slideshowSlider\"\r\n        style={{ transform: `translate3d(${-index * 100}%, 0, 0)` }}\r\n      >\r\n        {events.map((e, index) => (\r\n          <div\r\n            className=\"slide\"\r\n            key={index}\r\n            \r\n          ><SliderEvent key={index} event={e}/></div>\r\n        ))}\r\n        \r\n      </div>\r\n\r\n      <div className=\"slideshowDots\">\r\n        {events.map((_, idx) => (\r\n          <div\r\n            key={idx}\r\n            className={`slideshowDot${index === idx ? \" active\" : \"\"}`}\r\n            onClick={() => {\r\n              setIndex(idx);\r\n            }}\r\n          ></div>\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n\r\n\r\n\r\n}\r\n","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/sidebar/sidebar.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/singleEvent/SingleEvent.js",["140"],"import React,{useState,useEffect, useContext} from \"react\";\r\nimport axios from \"axios\";\r\nimport {useLocation, Link } from \"react-router-dom\";\r\nimport \"./singleEvent.css\";\r\nimport { Context } from \"../../context/Context\";\r\n\r\nconst SingleEvent = () => {\r\n\r\n  //Using window location to get the url\r\n  const location = useLocation();\r\n  const path = location.pathname.split(\"/\")[2];\r\n  const [event, setEvent] = useState({});\r\n\r\n  //fetching image from local host 5000\r\n  const PF = \"http://localhost:5000/images/\"\r\n\r\n  //Fetching user from context\r\n  const {user} = useContext(Context)\r\n\r\n  //Declaring set and use states\r\n  const [title, setTitle] = useState(\"\");\r\n  const [desc, setDesc] = useState(\"\");\r\n  const [date, setDate] = useState(\"\");\r\n  const [prize, setPrize] = useState(\"\");\r\n  const [venue, setVenue] = useState(\"\");\r\n\r\n  const [updateMode, setUpdateMode] = useState(false);\r\n\r\n  //It gets the value of the set variable\r\n  useEffect(()=>{\r\n    const getEvent = async ()=>{\r\n      const res = await axios.get(\"/events/\"+path);\r\n      setEvent(res.data);\r\n      setTitle(res.data.title);\r\n      setDesc(res.data.desc);\r\n      setDate(res.data.date);\r\n      setPrize(res.data.prize);\r\n      setVenue(res.data.venue);\r\n    };\r\n    getEvent()\r\n  },[path])\r\n\r\n\r\n//It handles the delete functionality of an event\r\n  const handleDelete = async ()=>{\r\n    try {\r\n      await axios.delete(`/events/${event._id}`,{\r\n        data:{username:user.username},\r\n      });\r\n      window.location.replace(\"/\");//after deleting it reloads the page\r\n\r\n    } catch (err) {\r\n      \r\n    }\r\n  }\r\n\r\n  //Handles the update event functionality of an event\r\n  const handleUpdate = async ()=>{\r\n    try {\r\n      await axios.put(`/events/${event._id}`,{\r\n        username:user.username, title,desc,date,prize,venue,\r\n      });\r\n      window.location.reload(\"/\");//after updating it reloads the page\r\n\r\n    } catch (err) {\r\n      \r\n    }\r\n  }\r\n  \r\n  return (\r\n      <div className=\"singleEvent\">\r\n        <div className=\"singleEventWrapper\">\r\n        {/* Fetching the data of a single event and displaying it */}\r\n          {event.photo && (\r\n            <img className=\"singleWrappereImg\"\r\n            src={PF + event.photo}\r\n            alt=\"\"\r\n            className=\"singleEventImg\"\r\n          />\r\n          )}{\r\n            updateMode ? <input type=\"text\" className=\"singleEventTitleInput\" value={title} autoFocus  onChange={(e)=>setTitle(e.target.value)}/> : (\r\n              <h1 className=\"singleEventTitle\">\r\n            {event.title}\r\n            {/* Only the admin who added the event can edit and delete the events */}\r\n            {event.username === user?.username && (\r\n              <div className=\"singleEventEdit\">\r\n              <i className=\"singleEventIcon far fa-edit\" onClick={()=>setUpdateMode(true)}></i>\r\n                <i className=\"singleEventIcon far fa-trash-alt\" onClick={handleDelete}></i>\r\n              </div>\r\n            )}\r\n          </h1>\r\n            )\r\n          }\r\n          {/* fetching the details like origanizer,venue and price */}\r\n          <div className=\"singleEventInfo\">\r\n            <span className=\"singleEventAuthor\">\r\n              Organized By :  \r\n              <Link to={`/?user=${event.username}`} className=\"link\"><b> {event.username} </b></Link>\r\n            </span>\r\n            <span className=\"singleEventDate\">Win:${event.prize}</span>\r\n            <span className=\"singleEventDate\">On : {new Date(event.date).toDateString()}</span>\r\n          </div>\r\n          {updateMode ? <input type=\"text\" className=\"singleEventVenueInput\" value={venue} autoFocus  onChange={(e)=>setVenue(e.target.value)}/> : (\r\n            <p className=\"singleEventVenue\">{event.venue}</p>\r\n          ) }\r\n          {/* if it is update mode then the admin can update the description of the event */}\r\n          {updateMode ? <textarea className=\"singleEventDescInput\" value={desc} onChange={(e)=>setDesc(e.target.value)}/> : (\r\n            <p className=\"singleEventDesc\">{event.desc}</p>\r\n          ) }\r\n          {updateMode && <button className=\"singleEventButton\" onClick={handleUpdate}> Update </button>}\r\n        </div>\r\n      </div>\r\n\r\n\r\n  );\r\n}\r\n\r\nexport default SingleEvent;","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/eventSidebar/eventSidebar.js",["141","142","143"],"import axios from \"axios\";\r\nimport { useEffect, useState } from \"react\"\r\nimport { Link } from \"react-router-dom\";\r\nimport \"./eventSidebar.css\"\r\nimport SearchIcon from '@mui/icons-material/Search';\r\nimport { Button } from \"@mui/material\";\r\n\r\nexport default function SidebarEve(){\r\n\r\n    //Declaring set and use states\r\n    const [date, setDate] = useState(\"\");\r\n\r\n    //handles date functionality\r\n    const handleDate = async()=>{\r\n        try {\r\n            window.location.replace(`/?date=${date}`);//query based on date\r\n        } catch (err) {\r\n\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"sidebarEveItems\">\r\n           <div className = \"searchByDateDiv\">\r\n               {/* form to search date */}\r\n            <form onSubmit={handleDate}>\r\n                {/* date */}\r\n                    <label className=\"sidebarEveTitle\">Search By Date : <input name=\"date\" type=\"date\" onChange={(e) => setDate(e.target.value)}></input><Button type=\"submit\"><SearchIcon style={{color:\"black\", fontSize:40}} /></Button></label>\r\n            </form>\r\n           </div>\r\n        </div>\r\n    )\r\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/singlePost/SinglePost.js",["144"],"import React,{useState,useEffect, useContext} from \"react\";\r\nimport axios from \"axios\";\r\nimport {useLocation, Link } from \"react-router-dom\";\r\nimport \"./singlePost.css\";\r\nimport { Context } from \"../../context/Context\";\r\n\r\nconst SinglePost = () => {\r\n\r\n  //fetching the url\r\n  const location = useLocation();\r\n  const path = location.pathname.split(\"/\")[2];\r\n  const [post, setPost] = useState({});\r\n\r\n  //fetching image from local host\r\n  const PF = \"http://localhost:5000/images/\"\r\n  //fetching user from context\r\n  const {user} = useContext(Context)\r\n\r\n  //Declaring set and use states\r\n  const [title, setTitle] = useState(\"\");\r\n  const [desc, setDesc] = useState(\"\");\r\n  const [updateMode, setUpdateMode] = useState(false);\r\n\r\n\r\n\r\n  //Fetchs a single post from the backend and sets it to the variable\r\n  useEffect(()=>{\r\n    const getPost = async ()=>{\r\n      const res = await axios.get(\"/posts/\"+path);\r\n      setPost(res.data);\r\n      setTitle(res.data.title);\r\n      setDesc(res.data.desc);\r\n    };\r\n    getPost()\r\n  },[path])\r\n\r\n\r\n  //Handles the delete functionality\r\n  const handleDelete = async ()=>{\r\n    try {\r\n      await axios.delete(`/posts/${post._id}`,{\r\n        data:{username:user.username},\r\n      });\r\n      window.location.replace(\"/\");//Reloads the page\r\n\r\n    } catch (err) {\r\n      \r\n    }\r\n  }\r\n\r\n  //Handles the update functionality\r\n  const handleUpdate = async ()=>{\r\n    try {\r\n      await axios.put(`/posts/${post._id}`,{\r\n        username:user.username, title,desc,\r\n      });\r\n      window.location.reload(\"/\");//Reloads the page\r\n\r\n    } catch (err) {\r\n      \r\n    }\r\n  }\r\n  \r\n  return (\r\n    <div>\r\n      <div className=\"singlePost\">\r\n        {/* fetching a single post and displaying in the page */}\r\n        <div className=\"singlePostWrapper\">\r\n          {post.photo && (\r\n            <img className=\"singleWrapperImg\"\r\n            src={PF + post.photo}\r\n            alt=\"\"\r\n            className=\"singlePostImg\"\r\n          />\r\n          )}{\r\n            updateMode ? <input type=\"text\" className=\"singlePostTitleInput\" value={title} autoFocus  onChange={(e)=>setTitle(e.target.value)}/> : (\r\n              <h1 className=\"singlePostTitle\">\r\n            {post.title}\r\n            {/* Only the user who added the blog can edit and delete the blog */}\r\n            {post.username === user?.username && (\r\n              <div className=\"singlePostEdit\">\r\n              <i className=\"singlePostIcon far fa-edit\" onClick={()=>setUpdateMode(true)}></i>\r\n                <i className=\"singlePostIcon far fa-trash-alt\" onClick={handleDelete}></i>\r\n              </div>\r\n            )}\r\n          </h1>\r\n            )\r\n          }\r\n        {/* fectching and displaying the details like author and created date */}\r\n          <div className=\"singlePostInfo\">\r\n            <span className=\"singlePostAuthor\">\r\n              Author:  \r\n              <Link to={`/?user=${post.username}`} className=\"link\"><b> {post.username} </b></Link>\r\n            </span>\r\n            <span className=\"singlePostDate\">{new Date(post.createdAt).toDateString()}</span>\r\n          </div>\r\n          {/* It can be updated only if it is in updated mode or it it will just show the content */}\r\n          {updateMode ? <textarea className=\"singlePostDescInput\" value={desc} onChange={(e)=>setDesc(e.target.value)}/> : (\r\n            <p className=\"singlePostDesc\">{post.desc}</p>\r\n          ) }\r\n          {/* onclick it handles the update funcion */}\r\n          {updateMode && <button className=\"singlePostButton\" onClick={handleUpdate}> Update </button>}\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  );\r\n}\r\n\r\nexport default SinglePost;","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/footer/Footer.js",["145"],"import React from 'react'\r\nimport \"./footer.css\"\r\nimport { useContext } from 'react'\r\nimport { Link } from \"react-router-dom\";\r\nimport { Context } from \"../../context/Context\";\r\nimport FacebookIcon from '@mui/icons-material/Facebook';\r\nimport InstagramIcon from '@mui/icons-material/Instagram';\r\nimport Button from '@material-ui/core/Button';\r\nimport EmailIcon from '@mui/icons-material/Email';\r\nimport TwitterIcon from '@mui/icons-material/Twitter';\r\n\r\nexport default function Footer() {\r\n\r\n    // Fetching user details from context\r\n    const {user, dispatch} = useContext(Context)\r\n\r\n    return (\r\n        <div className=\"footer\">\r\n            <div className=\"footerTitles\">\r\n            <ul className=\"botList\">\r\n                    <li className=\"botListItem\">\r\n                        {/* Link to Home page */}\r\n                        <Link className=\"link\" to=\"/\">HOME</Link> \r\n                    </li>\r\n                    {/* Query which shows the blogs written by the user */}\r\n                    {user ? (\r\n                        <li className=\"botListItem\">\r\n                            <Link className=\"link\" to={`/?user=${user.username}`}>BLOGS</Link>\r\n                        </li>\r\n                        ) : (\r\n                            <li className=\"botListItem\">\r\n                            <Link className=\"link\" to=\"/login\">BLOGS</Link>\r\n                            </li>\r\n                        )}\r\n                    <li className=\"botListItem\">\r\n                        {/* link to the evnets page */}\r\n                        <Link className=\"link\" to=\"/event\">EVENT</Link>\r\n                    </li>\r\n                    \r\n                    <li className=\"botListItem\">\r\n                        {/* Link to the about us page */}\r\n                    <Link className=\"link\" to=\"/about\">ABOUT</Link>\r\n                    </li>\r\n                    \r\n            </ul>\r\n            </div>\r\n\r\n\r\n            <div className='icons'>\r\n                {/* Contact us buttons */}\r\n                <div className = \"button\"><Button variant=\"outlined\" color=\"blue\" href=\"https://www.facebook.com/\">Facebook <FacebookIcon/></Button></div>\r\n                <div className = \"button\"><Button variant=\"outlined\" color=\"red\" href=\"https://www.twitter.com/\">Twitter <TwitterIcon/></Button></div>\r\n                <div className = \"button\"><Button variant=\"outlined\" color=\"black\" href=\"mailto:sampathkumarreddy.m19@iiits.in\">CONTACT US <EmailIcon/></Button></div>\r\n                <div className = \"button\"><Button variant=\"outlined\" color=\"pink\" href=\"https://www.instagram.com/?hl=en\">Instagram <InstagramIcon/></Button></div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/posts/Posts.js",[],"/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/navbar/NavBar.js",["146","147"],"import \"./navbar.css\"\r\nimport React, { useContext, useState} from 'react'\r\nimport logo from \"../../assets/Picture1.jpg\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { Context } from \"../../context/Context\";\r\n\r\n\r\nexport default function NavBar() {\r\n    //Fetching user details from context\r\n    const {user, dispatch} = useContext(Context)\r\n    const PF = \"http://localhost:5000/images/\"\r\n\r\n    //Handles logout functionality\r\n    const handleLogout = ()=>{\r\n        dispatch({type:\"LOGOUT\"});\r\n    };\r\n\r\n    return (\r\n        <div className=\"nav\">\r\n            <div id=\"logo\">\r\n                {/* Logo Image */}\r\n                <img src={logo} />\r\n            </div>\r\n\r\n            <div className=\"centerNav\">\r\n                <ul className=\"topList\">\r\n                    <li className=\"topListItem\">\r\n                        {/* Link to home page */}\r\n                        <Link className=\"link\" to=\"/\">HOME</Link>\r\n                    </li>\r\n                    {user ? (\r\n                        <li className=\"topListItem\">\r\n                            {/* query which gives us the blogs written by the user */}\r\n                        <Link className=\"link\" to={`/?user=${user.username}`}>BLOGS</Link>\r\n                        </li>\r\n                    ) : (\r\n                        <li className=\"topListItem\">\r\n                            {/* if not a user then redirected to login page */}\r\n                        <Link className=\"link\" to=\"/login\">BLOGS</Link>\r\n                        </li>\r\n                    )}\r\n                    <li className=\"topListItem\">\r\n                        {/* links to add blogs page */}\r\n                    <Link className=\"link\" to=\"/add\">ADD</Link>\r\n                    </li>\r\n                    <li className=\"topListItem\">\r\n                        {/* links to events page */}\r\n                        <Link className=\"link\" to=\"/event\">EVENT</Link>\r\n                    </li>\r\n                    <li className=\"topListItem\">\r\n                        {/* links to about us page */}\r\n                    <Link className=\"link\" to=\"/about\">ABOUT</Link>\r\n                    </li>\r\n\r\n                    {user ? (user.username===\"Sampath\" ? (\r\n                        <li className=\"topListItem\">\r\n                        {/* Admin page */}\r\n                    <Link className=\"link\" to=\"/AdminPage\">ADMIN PAGE</Link></li> ): (\" \")) : (\" \")}\r\n\r\n\r\n\r\n                    {/* on click it handles the logout functionality */}\r\n                    <li className=\"topListItem\" onClick={handleLogout}>\r\n                    {user && \"LOGOUT\"}\r\n                    </li>\r\n\r\n                </ul>\r\n            </div>\r\n            <div className=\"rightNav\">\r\n                { user ? (\r\n                    // if user is present it shows the profile pic at right side which is linked to settings\r\n                    <Link to=\"/settings\" className=\"link\"><img className=\"rightImg\" src={PF + user.profilePic} alt=\"\"/>\r\n                    <span className=\"userName\">{user.username}</span>\r\n                    </Link>\r\n                \r\n                    ) :(\r\n                        <ul className=\"topList\">\r\n                            <li className=\"topListItem\">\r\n                                {/* links to login page */}\r\n                                <Link className=\"link\" to=\"/login\">LOGIN</Link>\r\n                            </li>\r\n                            <li className=\"topListItem\">\r\n                                {/* links to register page */}\r\n                                <Link className=\"link\" to=\"/register\">REGISTER</Link>\r\n                            </li>\r\n                        </ul>\r\n                    )\r\n                }\r\n            </div>\r\n        </div>\r\n    )\r\n}","/Users/sampathkumarreddymanam/Documents/sem6/wbd/project-28-03/grp27/src/components/slider-event/SliderEvent.js",["148"],"import React